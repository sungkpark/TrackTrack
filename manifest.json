{
  "manifest_version": 2,
  "name": "TrackTrack",
  "version": "1.0",
  "description": "A time tracker that helps you use time screen time wisely",

  "icons": { "16": "img/icon16.png",
             "48": "img/icon48.png",
            "128": "img/icon128.png" },
  "browser_action": {
    "default_icon": "img/icon16.png" },
  "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["js/jquery-3.4.0.min.js", "js/content.js"]
      }
    ]
}
